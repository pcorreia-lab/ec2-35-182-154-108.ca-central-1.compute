                 "web_pool_2": {
                     "class": "Pool",
                     "monitors": [
                         "http"
                     ],
                     "members": [
                         {
                             "servicePort": 80,
                             "serverAddresses": [
                                 "192.168.2.10",
                                 "192.168.2.11"
                             ]
                         }
                     ]
                 }
             }
         }
     }
